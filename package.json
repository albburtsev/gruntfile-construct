{
  "name": "gruntfile-construct",
  "version": "0.0.0",
  "description": "Parser of gruntfiles, can be used for adding, deleting and updating grunt tasks",
  "main": "gruntfile-construct.js",
  "scripts": {
    "jshint": "jshint --reporter node_modules/jshint-stylish/stylish.js *.js test/*.js",
    "jscs": "jscs *.js test/*.js",
    "mocha": "mocha --reporter spec",
    "test": "npm run jshint && npm run jscs && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/albburtsev/gruntfile-construct.git"
  },
  "keywords": [
    "grunt",
    "tools",
    "gruntfile"
  ],
  "author": "Alexander Burtsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/albburtsev/gruntfile-construct/issues"
  },
  "homepage": "https://github.com/albburtsev/gruntfile-construct",
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "glob": "~3.2.7",
    "jscs": "~1.7.3",
    "jshint": "~2.5.10",
    "jshint-stylish": "~1.0.0",
    "mocha": "~1.13.0"
  },
  "dependencies": {
    "ast-types": "~0.3.8",
    "detect-indent": "~3.0.0",
    "lodash": "~2.2.1",
    "rocambole": "~0.3.6",
    "rocambole-token": "~1.2.1",
    "stringify-object": "~1.0.0",
    "underscore.string": "~2.4.0"
  }
}
